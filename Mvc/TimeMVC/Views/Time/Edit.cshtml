@model TimesMVC.Core.Entities.Time;

@{
    ViewData["Title"] = "Edit";
}

<h2>@ViewData["Title"]</h2>

<h4>Time</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="Nome" class="control-label"></label>
                <input asp-for="Nome" class="form-control" />
                <span asp-validation-for="Nome" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Fundado" class="control-label"></label>
                <input asp-for="Fundado" class="form-control" />
                <span asp-validation-for="Fundado" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Rival" class="control-label"></label>
                <input asp-for="Rival" class="form-control" />
                <span asp-validation-for="Rival" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Titulos" class="control-label"></label>
                <input asp-for="Titulos" class="form-control" />
                <span asp-validation-for="Titulos" class="text-danger"></span>
            </div>
            <div>
                <p> </p>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>
